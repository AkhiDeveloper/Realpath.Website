                                        <span class=icon fa fa-angle-down></span>
                                    </a>
                                    <ul class="dropdown-menu style-one" aria-labelledby="SiteMode_1">
                                        <li>
                                            <a href="#">Office Mode</a> 
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!--Header Upper-->
            <div class="header-upper">
                <div class="auto-container">
                    <div class="clearfix">
                        <!--Company Logo-->
                        <div class="d-flex justify-content-center logo-outer pb-3">
                                <a asp-area="" asp-page="Index">
                                    <div class="logo">
                                        <div class="logo-image spin">
                                            <img src="/RealPath Logo 2.png" alt="" title="" />
                                        </div>
                                        <div class="logo-text spin">
                                            <p class="spin" style="font-size:x-large;color:white; text-transform: uppercase"><span>R</span>ealpath Engineering Consultancy Pvt. Ltd.</p>
                                        </div>
                                     </div>
                                </a>
                        </div>
                    </div>
                </div>
            </div>

            <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="main-menu navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navigation navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Services/Index">Services</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Projects/Index">Projects</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Members/Index">Team</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Contactus/Index">Contact Us</a>
                        </li>
                    </ul>
                </div>
                
            </div>
         </nav>            
        </header>
    </div>
   
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

